# Generated by Chef for <%= node[:fqdn] %>

resource <%= @resource %> {
  device    <%= @device %>;
  disk      <%= @disk %>;
  meta-disk internal;
  on <%= @local_hostname %> {
    address   <%= @local_ip %>:<%= @port %>;
  }
  on <%= @remote_hostname %> {
    address   <%= @remote_ip %>:<%= @port %>;
  }
}
